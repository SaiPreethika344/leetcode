numbers = list(map(int, input("Enter numbers: ").split()))
positive = 0
negative = 0
zeros = 0

for num in numbers:
    if num > 0:
        positive += 1
    elif num < 0:
        negative += 1
    else:
        zeros += 1

print("Positive:", positive)
print("Negative:", negative)
print("Zeros:", zeros)
